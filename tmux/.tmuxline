# This tmux statusbar config was created by tmuxline.vim
# on Sat, 13 May 2017

set -g status-bg "colour14"
set -g message-command-fg "colour0"
set -g status-justify "left"
set -g status-left-length "100"
set -g status "on"
set -g pane-active-border-fg "colour14"
set -g message-bg "colour11"
set -g status-right-length "100"
set -g status-right-attr "none"
set -g message-fg "colour0"
set -g message-command-bg "colour11"
set -g status-attr "none"
set -g pane-border-fg "colour11"
set -g status-left-attr "none"
setw -g window-status-fg "colour0"
setw -g window-status-attr "none"
setw -g window-status-activity-bg "colour14"
setw -g window-status-activity-attr "none"
setw -g window-status-activity-fg "colour0"
setw -g window-status-separator ""
setw -g window-status-bg "colour14"
set -g status-left "#[fg=colour0,bg=colour14] #S #[fg=colour14,bg=colour14,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=colour14,bg=colour14,nobold,nounderscore,noitalics]#[fg=colour0,bg=colour14] %Y-%m-%d  %H:%M #[fg=colour14,bg=colour14,nobold,nounderscore,noitalics]#[fg=colour0,bg=colour14] #h "
setw -g window-status-format "#[fg=colour0,bg=colour14] #I #[fg=colour0,bg=colour14] #W "
setw -g window-status-current-format "#[fg=colour14,bg=colour11,nobold,nounderscore,noitalics]#[fg=colour0,bg=colour11] #I #[fg=colour0,bg=colour11] #W #[fg=colour11,bg=colour14,nobold,nounderscore,noitalics]"
